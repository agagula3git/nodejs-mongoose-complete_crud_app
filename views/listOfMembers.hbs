<div class="container">
<h2 class="text-white"><a class="btn btn-secondary" href="/insertMember"><i class ="fa fa-plus"></i>Create new</a> Member List</h2>
<table class="table table-dark table-striped">
  <tr>
      <th scope="col">Full Name</th>
      <th scope="col">Email</th>
      <th scope="col">Telephone</th>
      <th scope="col">Address</th>
      <th scope="col">Edit/Delete</th>
  </tr>
  <tbody>
      {{#each members}}
      <tr>
          <td>{{this.fullName}}</td>
          <td>{{this.email}}</td>
          <td>{{this.telephone}}</td>
          <td>{{this.address}}</td>
          <td>
                <a href="/editMember/{{this._id}}"><i class="fa fa-pencil fa-lg" aria-hidden="true"></i></a>
                <a href="/deleteMember/{{this._id}}" onclick="return confirm('Are you sure want to delete this item?');"><i class="fa fa-trash fa-lg" aria-hidden="true"></i></a>
            </td>
      </tr>
      {{/each}}
  </tbody>
</table>
</div>